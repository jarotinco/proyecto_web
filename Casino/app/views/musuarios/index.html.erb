<p id="notice"><%= notice %></p>

<h1>Musuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellidos</th>
      <th>Usuario</th>
      <th>Password</th>
      <th>Sala</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musuarios.each do |musuario| %>
      <tr>
        <td><%= musuario.nombre %></td>
        <td><%= musuario.apellidos %></td>
        <td><%= musuario.usuario %></td>
        <td><%= musuario.password %></td>
        <td><%= musuario.sala %></td>
        <td><%= musuario.tipo %></td>
        <td><%= link_to 'Show', musuario %></td>
        <td><%= link_to 'Edit', edit_musuario_path(musuario) %></td>
        <td><%= link_to 'Destroy', musuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Musuario', new_musuario_path %>
